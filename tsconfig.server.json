{
  "include": ["./server/**/*"],
  "compilerOptions": {
    "esModuleInterop": true,
    "isolatedModules": true,
    "lib": ["ESNext"],
    "module": "ESNext",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "outDir": "dist/server",
    "resolveJsonModule": true,
    "strict": false,
    "strictNullChecks": false,
    "target": "esnext",
    "allowSyntheticDefaultImports": true
  }
}
